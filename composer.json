{
    "name": "mokhosh/youtube-laravel-api",
    "description": "It is a wrapper over Youtube Api v3 which simplifies functionalities the laravel way.",
    "keywords": ["mokhosh", "YoutubeLaravelApi", "Youtube LiveStream", "php", "Youtube Backup", "Youtube Video", "Youtube Api V3", "laravel", "Oauth", "google api", "Youtube Channel"],
    "homepage": "https://github.com/mokhosh/laravel-youtube-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Mo Khosh",
            "email": "mskhoshnazar@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "google/apiclient": "^2.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Mokhosh\\YoutubeLaravelApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mokhosh\\YoutubeLaravelApi\\": "tests"
        }
    },
    "scripts": {
        "check-style":
            "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style":
            "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Mokhosh\\YoutubeLaravelApi\\YoutubeLaravelApiServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
